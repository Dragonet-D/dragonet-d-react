    "@umijs/test": "^3.2.19",
    "braft-editor": "^2.3.9",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "umi": "^3.2.19",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/plugin-sass": "^1.1.1",
    "recast": "^0.20.4"
  },
